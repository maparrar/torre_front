      }\n\n      function dragended(event) {\n        if (!event.active) simulation.alphaTarget(0);\n        event.subject.fx = null;\n        event.subject.fy = null;\n      }\n\n      return d3.drag()\n        .on(\"start\", dragstarted)\n        .on(\"drag\", dragged)\n        .on(\"end\", dragended);\n    };\n\n    const margin = {top: 10, right: 30, bottom: 30, left: 40},\n      width = viewerWidth - margin.left - margin.right,\n      height = viewerHeight - margin.top - margin.bottom;\n\n\n    const simulation = d3.forceSimulation(data.nodes)\n      .force(\"link\", d3.forceLink(data.links).id(d => d.id).distance(d => d.distance))\n      .force(\"charge\", d3.forceManyBody())\n      .force(\"center\", d3.forceCenter(width / 2, height / 2))\n    ;\n\n    const zoom = d3.zoom()\n      .on('zoom', (event) => {\n        svg.attr('transform', event.transform);\n      })\n      .scaleExtent([1, 40]);\n\n    const svg = d3.select(\"#viewer\")\n      .append(\"svg\")\n      .attr(\"width\", width + margin.left + margin.right)\n      .attr(\"height\", height + margin.top + margin.bottom)\n      .call(zoom)\n      .append(\"g\")\n    ;\n\n    const link = svg.append(\"g\")\n      .attr(\"stroke\", \"#cddc39\")\n      .attr(\"stroke-opacity\", 0.6)\n      .selectAll(\"line\")\n      .data(data.links)\n      .join(\"line\")\n      .attr(\"stroke-width\", d => Math.sqrt(d.value))\n    ;\n\n    const node = svg.append(\"g\")\n      .attr(\"stroke-width\", 1.5)\n      .selectAll(\"circle\")\n      .data(data.nodes)\n      .join(\"circle\")\n      .attr(\"r\", d => d.radius)\n      .attr(\"stroke\", d => d.stroke)\n      .attr(\"fill\", d => d.color)\n      .call(drag(simulation))\n      .style(\"cursor\", \"pointer\")\n      .on('click', props.onCLick)\n    ;\n\n    node.append(\"title\")\n      .text(d => {\n        let text = d.name;\n        if(d.compensation){\n          text += `\\n${d.organization.name}`;\n          text += `\\n~${d.compensation.amount.toLocaleString()} USD / mo`;\n        }else{\n          text += `\\n${d.professionalHeadline}`;\n        }\n        return text\n      }\n    );\n\n    node.append(\"image\")\n      .attr(\"xlink:href\", \"https://github.com/favicon.ico\")\n      .attr(\"x\", 16)\n      .attr(\"y\", 16)\n      .attr(\"width\", 100)\n      .attr(\"height\", 120)\n    ;\n\n    node.append(\"text\")\n      .attr(\"dx\", 12)\n      .attr(\"dy\", \".35em\")\n      .attr(\"color\", \"#ffffff\")\n      .text(d => d.name)\n    ;\n\n    simulation.on(\"tick\", () => {\n      link\n        .attr(\"x1\", d => d.source.x)\n        .attr(\"y1\", d => d.source.y)\n        .attr(\"x2\", d => d.target.x)\n        .attr(\"y2\", d => d.target.y);\n\n      node\n        .attr(\"cx\", d => d.x)\n        .attr(\"cy\", d => d.y);\n    });\n  };\n\n  return <div className={classes.wrapper}>\n    <div id=\"viewer\" className={classes.viewer} ref={div} />\n  </div>\n};\n\nexport default Graph;\n","/home/maparrar/code/torre/torre_front/src/components/Header/Header.js",[],"/home/maparrar/code/torre/torre_front/src/containers/Viewer/Graph/dataTools.js",["78"],"/home/maparrar/code/torre/torre_front/src/components/UI/Button/Button.js",[],"/home/maparrar/code/torre/torre_front/src/components/UI/Modal/Backdrop/Backdrop.js",[],"/home/maparrar/code/torre/torre_front/src/components/UI/Modal/Modal.js",[],"/home/maparrar/code/torre/torre_front/src/components/Comparator/Comparator.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":31,"column":5,"nodeType":"85","endLine":31,"endColumn":21,"suggestions":"86"},{"ruleId":"83","severity":1,"message":"84","line":43,"column":5,"nodeType":"85","endLine":43,"endColumn":17,"suggestions":"87"},{"ruleId":"83","severity":1,"message":"88","line":50,"column":6,"nodeType":"85","endLine":50,"endColumn":27,"suggestions":"89"},{"ruleId":"90","severity":1,"message":"91","line":4,"column":1,"nodeType":"92","endLine":4,"endColumn":40}